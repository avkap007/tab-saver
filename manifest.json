{
    "manifest_version": 3,
    "name": "Tab Saver",
    "version": "1.0",
    "description": "Save all open tabs to a file and close them safely",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
}
  